{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 10"],
        "node": "current"
      },
      "debug": true,
      "modules": false
    }]
  ],
  "plugins": [
    "syntax-dynamic-import",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ]
}
