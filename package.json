{
  "name": "pattern-lib",
  "version": "1.0.0",
  "description": "A collection of reusable UI components",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel",
    "eslint": "./node_modules/.bin/eslint",
    "webpack": "./node_modules/.bin/webpack",
    "start": "webpack-dev-server --hot --mode development --env env=dev",
    "prebuild": "rimraf dist",
    "build": "yarn webpack",
    "build:dev": "yarn build -- --mode development --env env=dev",
    "build:prod": "NODE_ENV=production yarn build -- --mode production --env env=prod",
    "build:lib": "rimraf lib && yarn lint && yarn babel ./src --out-dir ./lib",
    "lint": "yarn eslint 'src/**/*.js'",
    "watch": "yarn build -- --watch",
    "watch:dev": "yarn watch -- --env env=dev",
    "watch:prod": "yarn watch -- --env env=prod",
    "webpack-defaults": "webpack-defaults"
  },
  "keywords": [
    "front-end",
    "javascript",
    "pattern-library"
  ],
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/eslint-parser": "7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.13.9",
    "autoprefixer": "10.2.4",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "7.1.2",
    "css-loader": "5.1.1",
    "eslint": "7.21.0",
    "eslint-loader": "4.0.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.2.0",
    "image-webpack-loader": "7.0.1",
    "mini-css-extract-plugin": "1.3.9",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-color-function": "4.1.0",
    "postcss-css-variables": "0.17.0",
    "postcss-import": "14.0.0",
    "postcss-loader": "5.0.0",
    "rimraf": "3.0.2",
    "sass-loader": "11.0.1",
    "style-loader": "2.0.0",
    "stylelint": "13.11.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-webpack-plugin": "2.1.1",
    "terser-webpack-plugin": "5.1.1",
    "url-loader": "4.1.1",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-merge": "5.7.3"
  }
}
