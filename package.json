{
  "name": "pattern-lib",
  "version": "1.0.0",
  "description": "A collection of reusable UI components",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "start": "webpack-dev-server --hot --mode development --env.env=dev",
    "prebuild": "rimraf dist",
    "build": "yarn webpack",
    "build:dev": "yarn build -- --mode development --env.env=dev",
    "build:prod": "yarn build -- --mode production --env.env=prod",
    "watch": "yarn build -- --watch",
    "watch:dev": "yarn watch -- --env.env=dev",
    "watch:prod": "yarn watch -- --env.env=prod",
    "webpack-defaults": "webpack-defaults"
  },
  "keywords": [
    "front-end",
    "javascript",
    "pattern-library"
  ],
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "^0.28.7",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "0.8.1",
    "postcss-loader": "2.1.4",
    "rimraf": "^2.6.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "0.10.4",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "4.1.2"
  }
}
