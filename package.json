{
  "name": "pattern-lib",
  "version": "1.0.0",
  "description": "A collection of reusable UI components",
  "main": "lib/index.js",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "start": "webpack-dev-server --hot --env.env=dev",
    "prebuild": "rimraf dist",
    "build": "yarn webpack",
    "build:dev": "yarn build -- --env.env=dev",
    "build:prod": "yarn build -- --env.env=prod",
    "watch": "yarn build -- --watch",
    "watch:dev": "yarn watch -- --env.env=dev",
    "watch:prod": "yarn watch -- --env.env=prod",
    "webpack-defaults": "webpack-defaults"
  },
  "keywords": [
    "front-end",
    "javascript",
    "pattern-library"
  ],
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.7.2",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "^0.8.0",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
