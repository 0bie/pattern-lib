{
  "name": "pattern-lib",
  "version": "1.0.0",
  "description": "A collection of reusable UI components",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel",
    "eslint": "./node_modules/.bin/eslint",
    "webpack": "./node_modules/.bin/webpack",
    "start": "webpack-dev-server --hot --mode development --env.env=dev",
    "prebuild": "rimraf dist",
    "build": "yarn webpack",
    "build:dev": "yarn build -- --mode development --env.env=dev",
    "build:prod": "NODE_ENV=production yarn build -- --mode production --env.env=prod",
    "build:lib": "rimraf lib && yarn lint && yarn babel ./src --out-dir ./lib",
    "lint": "yarn eslint 'src/**/*.js'",
    "watch": "yarn build -- --watch",
    "watch:dev": "yarn watch -- --env.env=dev",
    "watch:prod": "yarn watch -- --env.env=prod",
    "webpack-defaults": "webpack-defaults"
  },
  "keywords": [
    "front-end",
    "javascript",
    "pattern-library"
  ],
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "autoprefixer": "9.3.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "1.0.1",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.5.0",
    "mini-css-extract-plugin": "0.4.4",
    "node-sass": "4.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-css-variables": "0.11.0",
    "postcss-loader": "3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  }
}
